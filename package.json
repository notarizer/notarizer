        "laravel-mix": "^2.0",
        "tailwindcss": "^0.7.2"
    },
    "dependencies": {
        "fast-sha256": "^1.1.0",
        "glob-all": "^3.1.0",
        "laravel-mix-tailwind": "^0.1.0",
        "node-forge": "^0.7.6",
        "purgecss-webpack-plugin": "^1.3.1"
    }
